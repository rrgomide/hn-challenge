# Example environment variables for Docker Compose
# Copy this file to .env and fill in your values

# Application
PORT=3000
NODE_ENV=production

# Database
MONGODB_URI_PROD=mongodb://mongodb:27017/hn-challenge-prod
MONGODB_URI_DEV=mongodb://localhost:27017/hn_challenge

# Authentication
JWT_SECRET=your-secure-jwt-secret-here

# AI Services (at least one required)
GOOGLE_GENERATIVE_AI_API_KEY=your-google-ai-key-here
OPENAI_API_KEY=your-openai-key-here